@article{RILIS-2012,
abstract = {The resonance ionization laser ion source (RILIS) produces beams for the majority of experiments at the ISOLDE on-line isotope separator. A substantial improvement in RILIS performance has been achieved through a series of upgrade steps: replacement of the copper vapor lasers by a Nd:YAG laser; replacement of the old homemade dye lasers by new commercial dye lasers; installation of a complementary Ti:Sapphire laser system. The combined dye and Ti:Sapphire laser system with harmonics is capable of generating beams at any wavelength in the range of 210-950 nm. In total, isotopes of 31 different elements have been selectively laser-ionized and separated at ISOLDE, including recently developed beams of samarium, praseodymium, polonium, and astatine.},
author = {Fedosseev, V. N. and Berg, L. E. and Fedorov, D. V. and Fink, D. and Launila, O. J. and Losito, R. and Marsh, B. A. and Rossel, R. E. and Rothe, S. and Seliverstov, M. D. and Sjdin, A. M. and Wendt, K. D.A.},
